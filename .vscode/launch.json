    "version": "0.2.0",
    "configurations": [
        { //& e:/programs/Windows/Python37/python.exe -m pipenv run python -m pymylibrary
            "name": "Python: pymylibrary",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "pythonPath": "${config:python.pythonPath}",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "env": { }, //"PIPENV_SUPPRESS_NESTED_WARNING":1,"PIPENV_VERBOSITY":-1
            "envFile": "${workspaceFolder}/.venv"
        },
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "pythonPath": "${config:python.pythonPath}",
            "cwd": "${workspaceFolder}",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": { }, //"PIPENV_SUPPRESS_NESTED_WARNING":1,"PIPENV_VERBOSITY":-1
            "envFile": "${workspaceFolder}/.venv"
        },
        {
            "name": "PyTest",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "pythonPath": "${config:python.pythonPath}",
            "module": "pytest",
            "args": [
                "-sv"
            ],
            "cwd": "${workspaceRoot}",
            "env": {},
            "envFile": "${workspaceRoot}/.venv",
            "console": "integratedTerminal"
        }
    ]
}